[run]
omit =
    */tests/*
    */test_*.py
    send_manual_message.py
    setup_test_data.py
    test_connection_debug.py
    main.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract
